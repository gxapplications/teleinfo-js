  },
  "homepage": "https://github.com/gxapplications/teleinformation",
  "keywords": [
    "teleinfo",
    "teleinformation",
    "edf",
    "erdf",
    "enedis"
  ],
  "engines": {
    "node": ">= 8.13.0",
    "npm": ">= 6.4.1"
  },
  "standard": {
    "env": {
      "node": true
    },
    "ignore": [
      "/docs/"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "serialport": "^9.0.0",
    "lodash.isequal": "^4.5.0",
    "lodash.reduce": "^4.6.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "precommit-hook": "3.0.0",
